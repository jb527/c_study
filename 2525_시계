#include <stdio.h>

int main(void)
{
    int h, m, t, c, ch, cm;
    scanf("%d %d %d", &h, &m, &t);
    ch = (m + t) / 60;
    cm = (m + t) % 60;
    m = cm;
    h = h + ch;
    if(h >= 24){
        h = h - 24;
        printf("%d %d", h, m);
    }
    else
    printf("%d %d", h, m);
    
    return 0;
}
